@startuml

skinparam state {
  BackgroundColor #faf4b4
  BorderColor #c90000
}

skinparam note {
  BackgroundColor #f5f5f5
}

skinparam linetype polyline

title Detalle Caso de Uso Abrir Newton Cotes Abierto n=0


DIFERENCIACION_NUMERICA_E_INTEGRACION-[#green]-> abrir_newton_cotes_a0
note on link
  **Usuario solicita:**
  - Abrir Newton Cotes Abierto n=0
end note

state abrir_newton_cotes_a0{
  state 1 as " "
  state 2 as " "
  state 3 as " "
  
  [*] -[#green]-> 1
  1 -[#green]-> 2
  note on link
      **Sistema requiere:**
      - Método newton cotes abierto n=0 conocido
  end note
  2 -[#green]-> 3
  note on link
    **Usuario Selecciona:**
    - Método newton cotes abierto n=0 conocido
  end note
 
  3 -[#green]-> [*]
}

abrir_newton_cotes_a0-[#green]-> :abrirNewtonCotesA0

:abrirNewtonCotesA0-[#green]-> NEWTON_COTES_A0

@enduml